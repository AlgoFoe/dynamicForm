{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "User",
  "type": "object",
  "properties": {
    "username": {
      "type": "string",
      "minLength": 3
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "age": {
      "type": "integer",
      "minimum": 0
    },
    "country": {
      "type": "string",
      "enum": ["USA", "Canada", "Mexico"]
    },
    "birthDate": {
      "type": "string",
      "format": "date"
    }
  },
  "required": ["username", "email", "country"]
}
